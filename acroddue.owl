        <rdfs:comment>Describes a Person born in a certain Place.</rdfs:comment>
        <rdfs:label xml:lang="en">bornIn</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://acrodhdk.github.io/acro#carriedBy -->

