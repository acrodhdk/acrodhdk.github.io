        <rdfs:label xml:lang="en">carriedBy</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://acrodhdk.github.io/acro#carries -->

    <owl:ObjectProperty rdf:about="https://acrodhdk.github.io/acro#carries">
