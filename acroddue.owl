        <rdfs:comment xml:lang="en">Describes that a Critic carries an Interpretation.</rdfs:comment>
        <rdfs:label xml:lang="en">carries</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://acrodhdk.github.io/acro#childOf -->

    <owl:ObjectProperty rdf:about="https://acrodhdk.github.io/acro#childOf">
        <rdfs:subPropertyOf rdf:resource="https://acrodhdk.github.io/acro#hasFamilialRelationship"/>
        <owl:inverseOf rdf:resource="https://acrodhdk.github.io/acro#parentOf"/>
        <rdfs:domain rdf:resource="https://acrodhdk.github.io/acro#Person"/>
        <rdfs:range rdf:resource="https://acrodhdk.github.io/acro#Person"/>
        <rdfs:comment xml:lang="en">Describes that a Person is the child of another.</rdfs:comment>
        <rdfs:label xml:lang="en">childOf</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://acrodhdk.github.io/acro#citedBy -->

    <owl:ObjectProperty rdf:about="https://acrodhdk.github.io/acro#citedBy">
        <rdfs:subPropertyOf rdf:resource="https://acrodhdk.github.io/acro#knownBy"/>
        <owl:inverseOf rdf:resource="https://acrodhdk.github.io/acro#cites"/>
        <rdfs:domain rdf:resource="https://acrodhdk.github.io/acro#Person"/>
        <rdfs:range rdf:resource="https://acrodhdk.github.io/acro#Critic"/>
        <rdfs:comment xml:lang="en">Describes that a Person&apos;s words/ideas/works have been cited by a Critic.</rdfs:comment>
        <rdfs:label xml:lang="en">citedBy</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://acrodhdk.github.io/acro#cites -->

    <owl:ObjectProperty rdf:about="https://acrodhdk.github.io/acro#cites">
        <rdfs:subPropertyOf rdf:resource="https://acrodhdk.github.io/acro#knows"/>
        <rdfs:domain rdf:resource="https://acrodhdk.github.io/acro#Critic"/>
        <rdfs:range rdf:resource="https://acrodhdk.github.io/acro#Person"/>
        <rdfs:comment xml:lang="en">Describes a Person&apos;s knowledge of another and their work by citing their words/ideas/works.</rdfs:comment>
        <rdfs:label xml:lang="en">cites</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://acrodhdk.github.io/acro#cityOf -->

    <owl:ObjectProperty rdf:about="https://acrodhdk.github.io/acro#cityOf">
        <rdfs:domain rdf:resource="https://acrodhdk.github.io/acro#City"/>
        <rdfs:range rdf:resource="https://acrodhdk.github.io/acro#State"/>
        <rdfs:comment xml:lang="en">Describes that a City is physically located inside a State.</rdfs:comment>
        <rdfs:label xml:lang="en">cityOf</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://acrodhdk.github.io/acro#comparedTo -->

    <owl:ObjectProperty rdf:about="https://acrodhdk.github.io/acro#comparedTo">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#SymmetricProperty"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://acrodhdk.github.io/acro#Artist"/>
                    <rdf:Description rdf:about="https://acrodhdk.github.io/acro#Artwork"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://acrodhdk.github.io/acro#Artist"/>
                    <rdf:Description rdf:about="https://acrodhdk.github.io/acro#Artwork"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment xml:lang="en">Describes the fact that two Classes are compared with each other. The compared Classes could be either Artists or Artworks.</rdfs:comment>
        <rdfs:label xml:lang="en">comparedTo</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://acrodhdk.github.io/acro#consideredBy -->

    <owl:ObjectProperty rdf:about="https://acrodhdk.github.io/acro#consideredBy">
        <owl:inverseOf rdf:resource="https://acrodhdk.github.io/acro#considers"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://acrodhdk.github.io/acro#Artist"/>
                    <rdf:Description rdf:about="https://acrodhdk.github.io/acro#Meaning_Component"/>
                    <rdf:Description rdf:about="https://acrodhdk.github.io/acro#Related_Artwork"/>
                    <rdf:Description rdf:about="https://acrodhdk.github.io/acro#Related_Interpretation"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="https://acrodhdk.github.io/acro#Interpretation"/>
        <rdfs:comment xml:lang="en">Describes that a series of fundamental elements are considered by an Interpretation. Such elements could be a Related Interpretation, a Related Artwork, an Artist or a Meaning Component.</rdfs:comment>
        <rdfs:label xml:lang="en">consideredBy</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://acrodhdk.github.io/acro#considers -->

    <owl:ObjectProperty rdf:about="https://acrodhdk.github.io/acro#considers">
        <rdfs:domain rdf:resource="https://acrodhdk.github.io/acro#Interpretation"/>
        <rdfs:range>
