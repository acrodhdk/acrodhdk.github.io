    <!-- https://acrodhdk.github.io/acro#ancestorOf -->

    <owl:ObjectProperty rdf:about="https://acrodhdk.github.io/acro#ancestorOf">
        <rdfs:subPropertyOf rdf:resource="https://acrodhdk.github.io/acro#hasFamilialRelationship"/>
        <owl:inverseOf rdf:resource="https://acrodhdk.github.io/acro#descendantOf"/>
        <rdfs:domain rdf:resource="https://acrodhdk.github.io/acro#Person"/>
        <rdfs:range rdf:resource="https://acrodhdk.github.io/acro#Person"/>
        <rdfs:comment xml:lang="en">Describes that a Person is the ancestor of another.</rdfs:comment>
        <rdfs:label xml:lang="en">ancestorOf</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://acrodhdk.github.io/acro#belongsTo -->

    <owl:ObjectProperty rdf:about="https://acrodhdk.github.io/acro#belongsTo">
        <owl:inverseOf rdf:resource="https://acrodhdk.github.io/acro#hasExponent"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://acrodhdk.github.io/acro#Artist"/>
                    <rdf:Description rdf:about="https://acrodhdk.github.io/acro#Artwork"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="https://acrodhdk.github.io/acro#Movement"/>
        <rdfs:comment xml:lang="en">Describes that an Artist or an Artwork belongs to a certain Style.</rdfs:comment>
        <rdfs:label xml:lang="en">belongsTo</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://acrodhdk.github.io/acro#birthplaceOf -->

    <owl:ObjectProperty rdf:about="https://acrodhdk.github.io/acro#birthplaceOf">
        <owl:inverseOf rdf:resource="https://acrodhdk.github.io/acro#bornIn"/>
        <rdfs:domain rdf:resource="https://acrodhdk.github.io/acro#Place"/>
        <rdfs:range rdf:resource="https://acrodhdk.github.io/acro#Person"/>
        <rdfs:comment>Describes the Place where a Person was born.</rdfs:comment>
        <rdfs:label xml:lang="en">birthplaceOf</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://acrodhdk.github.io/acro#bornIn -->

