#################################################################
#    Classes
#################################################################

###  https://acrodhdk.github.io/acro#Artist
acro:Artist rdf:type owl:Class ;
            rdfs:subClassOf acro:Person ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty acro:comparedTo ;
                              owl:allValuesFrom acro:Artist
                            ] ;
            rdfs:comment "A Person who created an Artwork."@en ;
            rdfs:label "Artist"@en .


###  https://acrodhdk.github.io/acro#Artwork
acro:Artwork rdf:type owl:Class ;
             rdfs:subClassOf acro:Creative_work ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty acro:createdBy ;
                               owl:someValuesFrom acro:Artist
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty acro:comparedTo ;
                               owl:allValuesFrom acro:Artwork
                             ] ;
             rdfs:comment "A Creative Work made by an Artist and analyzed by a Critic through an Interpretation."@en ;
             rdfs:label "Artwork"@en .


###  https://acrodhdk.github.io/acro#City
acro:City rdf:type owl:Class ;
          rdfs:subClassOf acro:Place ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty acro:cityOf ;
                            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                            owl:onClass acro:State
                          ] ;
          rdfs:comment "A Place referring to a built-up area with a name, defined boundaries, and local government. Note that it refers not only to cities (e.g. Milan), but also villages and towns."@en ;
          rdfs:label "City"@en .


###  https://acrodhdk.github.io/acro#Content
acro:Content rdf:type owl:Class ;
             rdfs:subClassOf acro:Meaning_Component ;
             rdfs:comment "A Meaning Component referring to the visual and conceptual elements included in the Main Artwork."@en ;
             rdfs:label "Content"@en .


###  https://acrodhdk.github.io/acro#Context
acro:Context rdf:type owl:Class ;
             rdfs:subClassOf acro:Meaning_Component ;
             rdfs:comment "Meaning Component referring to circumstantial elements related to the Main Artist and the Main Artwork."@en ;
             rdfs:label "Context"@en .


###  https://acrodhdk.github.io/acro#Creative_work
acro:Creative_work rdf:type owl:Class ;
                   rdfs:comment "Top Class representing a most generic manifestation of creative effort."@en ;
                   rdfs:label "Creative Work"@en .


###  https://acrodhdk.github.io/acro#Critic
acro:Critic rdf:type owl:Class ;
            rdfs:subClassOf acro:Person ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty acro:carries ;
                              owl:someValuesFrom acro:Interpretation
                            ] ;
            rdfs:comment "A Person who analyzes an Artwork through an Interpretation."@en ;
            rdfs:label "Critic"@en .


###  https://acrodhdk.github.io/acro#Event
acro:Event rdf:type owl:Class ;
           rdfs:subClassOf acro:Context ;
           rdfs:comment "A kind of Context representing an important occurrence that happened and influenced the creation of the Main Artwork."@en ;
           rdfs:label "Event"@en .


###  https://acrodhdk.github.io/acro#Institution
acro:Institution rdf:type owl:Class ;
                 rdfs:subClassOf acro:Place ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty acro:institutionOf ;
                                   owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                   owl:onClass acro:City
                                 ] ;
                 rdfs:comment "A Place where an organization founded for a religious, educational, professional, or social purpose is physically located."@en ;
                 rdfs:label "Institution"@en .


###  https://acrodhdk.github.io/acro#Interpretation
acro:Interpretation rdf:type owl:Class ;
                    rdfs:subClassOf acro:Creative_work ;
                    rdfs:comment "A Creative Work made by a Critic that analyzes an Artwork."@en ;
                    rdfs:label "Interpretation"@en .


###  https://acrodhdk.github.io/acro#Main_Artist
acro:Main_Artist rdf:type owl:Class ;
                 rdfs:subClassOf acro:Artist ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty acro:creatorOf ;
                                   owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                   owl:onClass acro:Main_Artwork
                                 ] ;
                 rdfs:comment "The Artist who created the Main Artwork."@en ;
                 rdfs:label "Main Artist"@en .


###  https://acrodhdk.github.io/acro#Main_Artwork
acro:Main_Artwork rdf:type owl:Class ;
                  rdfs:subClassOf acro:Artwork ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty acro:objectOf ;
                                    owl:someValuesFrom acro:Main_Interpretation
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty acro:createdBy ;
                                    owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                    owl:onClass acro:Main_Artist
                                  ] ;
                  rdfs:comment "The Artwork made by the Main Artist and analyzed by the Main Critic through the Main Interpretation."@en ;
                  rdfs:label "Main Artwork"@en .


###  https://acrodhdk.github.io/acro#Main_Critic
acro:Main_Critic rdf:type owl:Class ;
                 rdfs:subClassOf acro:Critic ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty acro:carries ;
                                   owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                   owl:onClass acro:Main_Interpretation
                                 ] ;
                 rdfs:comment "The Critic who analyzes the Main Artwork through the Main Interpretation."@en ;
                 rdfs:label "Main Critic"@en .


###  https://acrodhdk.github.io/acro#Main_Interpretation
acro:Main_Interpretation rdf:type owl:Class ;
                         rdfs:subClassOf acro:Interpretation ,
                                         [ rdf:type owl:Restriction ;
                                           owl:onProperty acro:hasObject ;
                                           owl:allValuesFrom acro:Main_Artwork
                                         ] ,
                                         [ rdf:type owl:Restriction ;
                                           owl:onProperty acro:carriedBy ;
                                           owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                           owl:onClass acro:Main_Critic
                                         ] ;
                         rdfs:comment "The principal Interpretation, made by the Main Critic, that analyzes the Main Artwork by considering a series of aesthetic and contextual elements."@en ;
                         rdfs:label "Main Interpretation"@en .


###  https://acrodhdk.github.io/acro#Meaning_Component
acro:Meaning_Component rdf:type owl:Class ;
                       rdfs:subClassOf [ rdf:type owl:Restriction ;
                                         owl:onProperty acro:hasReference ;
                                         owl:someValuesFrom acro:Artwork
                                       ] ;
                       rdfs:comment """Top Class representing a conceptual construct about an Artwork, that makes it
understandable to other individuals."""@en ;
                       rdfs:label "Meaning Component"@en .


###  https://acrodhdk.github.io/acro#Movement
acro:Movement rdf:type owl:Class ;
              rdfs:subClassOf acro:Context ;
              rdfs:comment "A kind of Context that refers to an artistic style with a specific aesthetic."@en ;
              rdfs:label "Style"@en .


###  https://acrodhdk.github.io/acro#Person
acro:Person rdf:type owl:Class ;
            rdfs:comment "Top Class referring to any kind of human individual involved somehow in the development of the Main Interpretation."@en ;
            rdfs:label "Person"@en .


###  https://acrodhdk.github.io/acro#Place
acro:Place rdf:type owl:Class ;
           rdfs:comment "Top Class representing a physical location extended in space. It could be a country, a region, a city, a town, a building, and so on. It is possible to create a chain of subPlaces and superPlaces through the use of \"setIn\" property." ;
           rdfs:label "Place"@en .


###  https://acrodhdk.github.io/acro#Related_Artist
acro:Related_Artist rdf:type owl:Class ;
                    rdfs:subClassOf acro:Artist ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty acro:creatorOf ;
                                      owl:someValuesFrom acro:Artwork
                                    ] ;
                    rdfs:comment "An Artist who created an Artwork and that somehow is put in relation with the Main Artist."@en ;
                    rdfs:label "Related Artist"@en .


###  https://acrodhdk.github.io/acro#Related_Artwork
acro:Related_Artwork rdf:type owl:Class ;
                     rdfs:subClassOf acro:Artwork ;
                     rdfs:comment "An Artwork made by an Artist and put in relation with the Main Artwork. Note that the Artist could be either the Main Artist or a Related Artist."@en ;
                     rdfs:label "Related Artwork"@en .


###  https://acrodhdk.github.io/acro#Related_Critic
acro:Related_Critic rdf:type owl:Class ;
                    rdfs:subClassOf acro:Critic ;
                    rdfs:comment "A Critic who analyzes the Main Artwork through a Related Interpretation."@en ;
                    rdfs:label "Related Critic"@en .


###  https://acrodhdk.github.io/acro#Related_Interpretation
acro:Related_Interpretation rdf:type owl:Class ;
                            rdfs:subClassOf acro:Interpretation ,
                                            [ rdf:type owl:Restriction ;
                                              owl:onProperty acro:carriedBy ;
                                              owl:someValuesFrom acro:Critic
                                            ] ;
                            rdfs:comment "An Interpretation made by a Related Critic and put in relation with the Main Interpretation by the Main Critic."@en ;
                            rdfs:label "Related Interpretation"@en .


###  https://acrodhdk.github.io/acro#State
acro:State rdf:type owl:Class ;
           rdfs:subClassOf acro:Place ;
           rdfs:comment "A Place representing a nation or territory considered as an organized political community under one government."@en ;
           rdfs:label "State"@en .


###  https://acrodhdk.github.io/acro#Subject_Matter
acro:Subject_Matter rdf:type owl:Class ;
                    rdfs:subClassOf acro:Content ;
                    rdfs:comment "The visually recognizable Content in the Main Artwork."@en ;
                    rdfs:label "Subject Matter"@en .


###  https://acrodhdk.github.io/acro#Topic
acro:Topic rdf:type owl:Class ;
           rdfs:subClassOf acro:Content ;
           rdfs:comment "The kind of Content representing a conceptual theme presented by the Main Artwork."@en ;
           rdfs:label "Theme"@en .


