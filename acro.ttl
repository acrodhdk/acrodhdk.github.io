#################################################################
#    Object Properties
#################################################################

###  https://acrodhdk.github.io/acro#ancestorOf
acro:ancestorOf rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf acro:hasFamilialRelationship ;
                owl:inverseOf acro:descendantOf ;
                rdfs:domain acro:Person ;
                rdfs:range acro:Person ;
                rdfs:comment "Describes that a Person is the ancestor of another."@en ;
                rdfs:label "ancestorOf"@en .


###  https://acrodhdk.github.io/acro#belongsTo
acro:belongsTo rdf:type owl:ObjectProperty ;
               owl:inverseOf acro:hasExponent ;
               rdfs:domain [ rdf:type owl:Class ;
                             owl:unionOf ( acro:Artist
                                           acro:Artwork
                                         )
                           ] ;
               rdfs:range acro:Movement ;
               rdfs:comment "Describes that an Artist or an Artwork belongs to a certain Style."@en ;
               rdfs:label "belongsTo"@en .


###  https://acrodhdk.github.io/acro#birthplaceOf
acro:birthplaceOf rdf:type owl:ObjectProperty ;
                  owl:inverseOf acro:bornIn ;
                  rdfs:domain acro:Place ;
                  rdfs:range acro:Person ;
                  rdfs:comment "Describes the Place where a Person was born." ;
                  rdfs:label "birthplaceOf"@en .


###  https://acrodhdk.github.io/acro#bornIn
acro:bornIn rdf:type owl:ObjectProperty ;
            rdfs:domain acro:Person ;
            rdfs:range acro:Place ;
            rdfs:comment "Describes a Person born in a certain Place." ;
            rdfs:label "bornIn"@en .


###  https://acrodhdk.github.io/acro#carriedBy
acro:carriedBy rdf:type owl:ObjectProperty ;
               owl:inverseOf acro:carries ;
               rdfs:domain acro:Interpretation ;
               rdfs:range acro:Critic ;
               rdfs:comment "Describes that an Interpretation is carried by a Critic."@en ;
               rdfs:label "carriedBy"@en .


###  https://acrodhdk.github.io/acro#carries
acro:carries rdf:type owl:ObjectProperty ;
             rdfs:domain acro:Critic ;
             rdfs:range acro:Interpretation ;
             rdfs:comment "Describes that a Critic carries an Interpretation."@en ;
             rdfs:label "carries"@en .


###  https://acrodhdk.github.io/acro#childOf
acro:childOf rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf acro:hasFamilialRelationship ;
             owl:inverseOf acro:parentOf ;
             rdfs:domain acro:Person ;
             rdfs:range acro:Person ;
             rdfs:comment "Describes that a Person is the child of another."@en ;
             rdfs:label "childOf"@en .


###  https://acrodhdk.github.io/acro#citedBy
acro:citedBy rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf acro:knownBy ;
             owl:inverseOf acro:cites ;
             rdfs:domain acro:Person ;
             rdfs:range acro:Critic ;
             rdfs:comment "Describes that a Person's words/ideas/works have been cited by a Critic."@en ;
             rdfs:label "citedBy"@en .


###  https://acrodhdk.github.io/acro#cites
acro:cites rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf acro:knows ;
           rdfs:domain acro:Critic ;
           rdfs:range acro:Person ;
           rdfs:comment "Describes a Person's knowledge of another and their work by citing their words/ideas/works."@en ;
           rdfs:label "cites"@en .


###  https://acrodhdk.github.io/acro#cityOf
acro:cityOf rdf:type owl:ObjectProperty ;
            rdfs:domain acro:City ;
            rdfs:range acro:State ;
            rdfs:comment "Describes that a City is physically located inside a State."@en ;
            rdfs:label "cityOf"@en .


###  https://acrodhdk.github.io/acro#comparedTo
acro:comparedTo rdf:type owl:ObjectProperty ,
                         owl:SymmetricProperty ;
                rdfs:domain [ rdf:type owl:Class ;
                              owl:unionOf ( acro:Artist
                                            acro:Artwork
                                          )
                            ] ;
                rdfs:range [ rdf:type owl:Class ;
                             owl:unionOf ( acro:Artist
                                           acro:Artwork
                                         )
                           ] ;
                rdfs:comment "Describes the fact that two Classes are compared with each other. The compared Classes could be either Artists or Artworks."@en ;
                rdfs:label "comparedTo"@en .


###  https://acrodhdk.github.io/acro#consideredBy
acro:consideredBy rdf:type owl:ObjectProperty ;
                  owl:inverseOf acro:considers ;
                  rdfs:domain [ rdf:type owl:Class ;
                                owl:unionOf ( acro:Artist
                                              acro:Meaning_Component
                                              acro:Related_Artwork
                                              acro:Related_Interpretation
                                            )
                              ] ;
                  rdfs:range acro:Interpretation ;
                  rdfs:comment "Describes that a series of fundamental elements are considered by an Interpretation. Such elements could be a Related Interpretation, a Related Artwork, an Artist or a Meaning Component."@en ;
                  rdfs:label "consideredBy"@en .


###  https://acrodhdk.github.io/acro#considers
acro:considers rdf:type owl:ObjectProperty ;
               rdfs:domain acro:Interpretation ;
               rdfs:range [ rdf:type owl:Class ;
                            owl:unionOf ( acro:Artist
                                          acro:Meaning_Component
                                          acro:Related_Artwork
                                          acro:Related_Interpretation
                                        )
                          ] ;
               rdfs:comment "Describes that an Interpretation considers a series of fundamental elements. Such elements could be a Related Interpretation, a Related Artwork, an Artist or a Meaning Component."@en ;
               rdfs:label "considers"@en .


###  https://acrodhdk.github.io/acro#createdBy
acro:createdBy rdf:type owl:ObjectProperty ;
               owl:inverseOf acro:creatorOf ;
               rdfs:domain acro:Artwork ;
               rdfs:range acro:Artist ;
               rdfs:comment "Describes that an Artwork is created by an Artist."@en ;
               rdfs:label "createdBy"@en .


###  https://acrodhdk.github.io/acro#creationPlaceOf
acro:creationPlaceOf rdf:type owl:ObjectProperty ;
                     owl:inverseOf acro:locationCreated ;
                     rdfs:domain acro:Place ;
                     rdfs:range acro:Artwork ;
                     rdfs:comment "Describes that an Artwork was created in a certain Place"@en ;
                     rdfs:label "creationPlaceOf"@en .


###  https://acrodhdk.github.io/acro#creatorOf
acro:creatorOf rdf:type owl:ObjectProperty ;
               rdfs:domain acro:Artist ;
               rdfs:range acro:Artwork ;
               rdfs:comment "Describes that an Artist created an Artwork."@en ;
               rdfs:label "creatorOf"@en .


###  https://acrodhdk.github.io/acro#currentLocationOf
acro:currentLocationOf rdf:type owl:ObjectProperty ;
                       owl:inverseOf acro:hasCurrentLocation ;
                       rdfs:domain acro:Place ;
                       rdfs:range acro:Artwork ;
                       rdfs:comment "Describes that a Place is the current location of a certain Artwork."@en ;
                       rdfs:label "currentLocationOf"@en .


###  https://acrodhdk.github.io/acro#deathplaceOf
acro:deathplaceOf rdf:type owl:ObjectProperty ;
                  owl:inverseOf acro:diedIn ;
                  rdfs:domain acro:Place ;
                  rdfs:range acro:Person ;
                  rdfs:comment "Describes a Person that died in a certain Place." ;
                  rdfs:label "deathplaceOf"@en .


###  https://acrodhdk.github.io/acro#descendantOf
acro:descendantOf rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf acro:hasFamilialRelationship ;
                  rdfs:domain acro:Person ;
                  rdfs:range acro:Person ;
                  rdfs:comment "Describes that a Person is the descendant of another."@en ;
                  rdfs:label "descendantOf"@en .


###  https://acrodhdk.github.io/acro#diedIn
acro:diedIn rdf:type owl:ObjectProperty ;
            rdfs:domain acro:Person ;
            rdfs:range acro:Place ;
            rdfs:comment "Describes the Place where a certain Person has died." ;
            rdfs:label "diedIn"@en .


###  https://acrodhdk.github.io/acro#eventLocationOf
acro:eventLocationOf rdf:type owl:ObjectProperty ;
                     owl:inverseOf acro:hasEventLocation ;
                     rdfs:domain acro:Place ;
                     rdfs:range acro:Event ;
                     rdfs:comment "Describes that a Place is the location where a certain Event happened."@en ;
                     rdfs:label "eventLocationOf"@en .


###  https://acrodhdk.github.io/acro#friendOf
acro:friendOf rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf acro:knows ;
              rdf:type owl:SymmetricProperty ;
              rdfs:domain acro:Person ;
              rdfs:range acro:Person ;
              rdfs:comment "Describes that a Person is a friend with another."@en ;
              rdfs:label "friendOf"@en .


###  https://acrodhdk.github.io/acro#hasCurrentLocation
acro:hasCurrentLocation rdf:type owl:ObjectProperty ;
                        rdfs:domain acro:Artwork ;
                        rdfs:range acro:Place ;
                        rdfs:comment "Describes the current Place where a certain Artwork currently is." ;
                        rdfs:label "hasCurrentLocation"@en .


###  https://acrodhdk.github.io/acro#hasEventLocation
acro:hasEventLocation rdf:type owl:ObjectProperty ;
                      rdfs:domain acro:Event ;
                      rdfs:range acro:Place ;
                      rdfs:comment "Describes that an Event happened in a certain Place."@en ;
                      rdfs:label "hasEventLocation"@en .


###  https://acrodhdk.github.io/acro#hasExponent
acro:hasExponent rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf owl:topObjectProperty ;
                 rdfs:domain acro:Movement ;
                 rdfs:range [ rdf:type owl:Class ;
                              owl:unionOf ( acro:Artist
                                            acro:Artwork
                                          )
                            ] ;
                 rdfs:comment "Describes that a Style has a certain Artist or Artwork as an exponent."@en ;
                 rdfs:label "hasExponent"@en .


###  https://acrodhdk.github.io/acro#hasFamilialRelationship
acro:hasFamilialRelationship rdf:type owl:ObjectProperty ;
                             rdfs:domain acro:Person ;
                             rdfs:range acro:Person ;
                             rdfs:comment "Describes that a Person has a familial relationship with another."@en ;
                             rdfs:label "hasFamilialRelationship"@en .


###  https://acrodhdk.github.io/acro#hasHeardOf
acro:hasHeardOf rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf acro:knows ;
                rdf:type owl:AsymmetricProperty ;
                rdfs:domain acro:Person ;
                rdfs:range acro:Person ;
                rdfs:comment "Describes that a Person has heard of another."@en ;
                rdfs:label "hasHeardOf"@en .


###  https://acrodhdk.github.io/acro#hasObject
acro:hasObject rdf:type owl:ObjectProperty ;
               owl:inverseOf acro:objectOf ;
               rdfs:domain acro:Interpretation ;
               rdfs:range acro:Artwork ;
               rdfs:comment "Describes that an Interpretation has the Main Artwork as its object of analysis."@en ;
               rdfs:label "hasObject"@en .


###  https://acrodhdk.github.io/acro#hasReference
acro:hasReference rdf:type owl:ObjectProperty ,
                           owl:SymmetricProperty ;
                  rdfs:domain [ rdf:type owl:Class ;
                                owl:unionOf ( acro:Meaning_Component
                                              acro:Person
                                              acro:Place
                                            )
                              ] ;
                  rdfs:range [ rdf:type owl:Class ;
                               owl:unionOf ( acro:Meaning_Component
                                             acro:Person
                                             acro:Place
                                           )
                             ] ;
                  rdfs:comment "Describes that either a Meaning Component, a Place or a Person references to either another Meaning Component, a Place or a Person." ;
                  rdfs:label "hasReference"@en .


###  https://acrodhdk.github.io/acro#hasSubject
acro:hasSubject rdf:type owl:ObjectProperty ;
                owl:inverseOf acro:subjectOf ;
                rdfs:domain acro:Artwork ;
                rdfs:range acro:Subject_Matter ;
